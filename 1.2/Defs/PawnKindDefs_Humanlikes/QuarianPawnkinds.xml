<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!-- ============================ QuarianBase ========================== -->
  <PawnKindDef Name="QuarianBase" Abstract="True">
    <defaultFactionType>QuarianFaction</defaultFactionType>
    <race>AlienQuarian</race>
    <chemicalAddictionChance>0.05</chemicalAddictionChance>
    <baseRecruitDifficulty>0.85</baseRecruitDifficulty>
    <backstoryFiltersOverride>
      <li>
        <categories>
          <li>QuarianBackstory</li>
        </categories>
      </li>
    </backstoryFiltersOverride>
  </PawnKindDef>
  <!-- ============================ Poor Quarians ========================== -->
  <PawnKindDef Name="QuarianPoorBase" ParentName="QuarianBase" Abstract="True">
    <combatPower>30</combatPower>
    <race>AlienQuarian</race>
    <itemQuality>Poor</itemQuality>
    <isFighter>false</isFighter>
    <gearHealthRange>
      <min>0.2</min>
      <max>2.0</max>
    </gearHealthRange>
    <weaponMoney>
      <min>150</min>
      <max>180</max>
    </weaponMoney>
    <weaponTags>
      <li>MedievalMeleeBasic</li>
    </weaponTags>
    <invNutrition>1.0</invNutrition>
    <inventoryOptions>
      <skipChance>0.75</skipChance>
      <subOptionsChooseOne>
        <li>
          <choiceChance>50</choiceChance>
          <thingDef>Silver</thingDef>
          <countRange>
            <min>25</min>
            <max>70</max>
          </countRange>
        </li>
      </subOptionsChooseOne>
    </inventoryOptions>
  </PawnKindDef>
  <PawnKindDef ParentName="QuarianPoorBase">
    <defName>QuarianSlave01</defName>
    <label>slave</label>
  </PawnKindDef>
  <!-- ============================ Colonist Quarians ========================== -->
  <PawnKindDef Name="QuarianColonistBase" ParentName="QuarianBase" Abstract="True">
    <combatPower>40</combatPower>
    <race>AlienQuarian</race>
    <gearHealthRange>
      <min>0.5</min>
      <max>2.0</max>
    </gearHealthRange>
    <weaponMoney>
      <min>150</min>
      <max>300</max>
    </weaponMoney>
    <weaponTags>
      <li>Gun</li>
      <li>MedievalMeleeBasic</li>
    </weaponTags>
    <invNutrition>1.0</invNutrition>
    <inventoryOptions>
      <skipChance>0.75</skipChance>
      <subOptionsChooseOne>
        <li>
          <choiceChance>50</choiceChance>
          <thingDef>Silver</thingDef>
          <countRange>
            <min>50</min>
            <max>120</max>
          </countRange>
        </li>
      </subOptionsChooseOne>
    </inventoryOptions>
  </PawnKindDef>
  <PawnKindDef ParentName="QuarianColonistBase">
    <defName>QuarianColonist01</defName>
    <label>colonist</label>
  </PawnKindDef>
  <!-- ============================ Soldier Quarians ========================== -->
  <PawnKindDef Name="QuarianSoldierBase" ParentName="QuarianBase" Abstract="True">
    <combatPower>65</combatPower>
    <race>AlienQuarian</race>
    <gearHealthRange>
      <min>0.7</min>
      <max>2.0</max>
    </gearHealthRange>
    <weaponMoney>
      <min>300</min>
      <max>600</max>
    </weaponMoney>
    <weaponTags>
      <li>Gun</li>
    </weaponTags>
    <invNutrition>1.0</invNutrition>
  </PawnKindDef>
  <PawnKindDef ParentName="QuarianSoldierBase">
    <defName>QuarianSoldier01</defName>
    <label>soldier</label>
  </PawnKindDef>
  <!-- ============================ Veteran Quarians ========================== -->
  <PawnKindDef Name="QuarianVeteranBase" ParentName="QuarianBase" Abstract="True">
    <combatPower>90</combatPower>
    <race>AlienQuarian</race>
    <gearHealthRange>
      <min>0.9</min>
      <max>2.0</max>
    </gearHealthRange>
    <apparelMoney>
      <min>0</min>
      <max>1500</max>
    </apparelMoney>
    <apparelTags>
      <li>QuarianMilitary</li>
      <li>BeltDefensePop</li>
      <li>RangedShieldBelt</li>
    </apparelTags>
    <weaponMoney>
      <min>650</min>
      <max>1000</max>
    </weaponMoney>
    <weaponTags>
      <li>Gun</li>
      <li>SpacerGun</li>
    </weaponTags>
    <invNutrition>1.0</invNutrition>
    <inventoryOptions>
      <subOptionsChooseOne>
        <li>
          <choiceChance>20</choiceChance>
          <thingDef>MedicineIndustrial</thingDef>
          <countRange>
            <min>0</min>
            <max>2</max>
          </countRange>
        </li>
      </subOptionsChooseOne>
    </inventoryOptions>
  </PawnKindDef>
  <PawnKindDef ParentName="QuarianVeteranBase">
    <defName>QuarianVeteran01</defName>
    <label>veteran soldier</label>
  </PawnKindDef>
