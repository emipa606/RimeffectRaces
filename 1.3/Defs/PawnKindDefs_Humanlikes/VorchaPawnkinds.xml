  <!-- ============================ Poor Vorchas ========================== -->

	<PawnKindDef Name="VorchaPoorBase" ParentName="VorchaBase" Abstract="True">
		<combatPower>45</combatPower>
		<race>AlienVorcha</race>
		<initialWillRange>2~6</initialWillRange>
		<initialResistanceRange>12~17</initialResistanceRange>
		<itemQuality>Poor</itemQuality>
		<isFighter>false</isFighter>
		<gearHealthRange>
			<min>0.2</min>
			<max>2.0</max>
		</gearHealthRange>
		<apparelMoney>
			<min>300</min>
			<max>900</max>
		</apparelMoney>
		<apparelAllowHeadgearChance>0.3</apparelAllowHeadgearChance>
		<apparelTags>
			<li>Neolithic</li>
		</apparelTags>
		<weaponMoney>
			<min>150</min>
			<max>180</max>
		</weaponMoney>
		<weaponTags>
			<li>Gun</li>
			<li>MedievalMeleeBasic</li>
		</weaponTags>
		<inventoryOptions>
			<subOptionsChooseOne>
