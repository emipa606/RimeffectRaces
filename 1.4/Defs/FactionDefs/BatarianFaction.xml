<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <FactionDef ParentName="FactionBase">
    <defName>BatarianFaction</defName>
    <label>Batarian Expatriates</label>
    <description>A batarian slave company used to have one of their largest warehouses on this planet, until a law enforcement presence forced them into hiding for decades. Due to that the batarians had to stay on this planet and after a while they just settled here, forgetting about their space piracy ways.</description>
    <pawnsPlural>Batarian</pawnsPlural>
    <fixedName>Batarian Expatriates</fixedName>
    <allowedCultures>
      <li>BatarianCulture</li>
    </allowedCultures>
    <settlementGenerationWeight>1</settlementGenerationWeight>
    <requiredCountAtGameStart>1</requiredCountAtGameStart>
    <maxConfigurableAtWorldCreation>9999</maxConfigurableAtWorldCreation>
    <configurationListOrderPriority>10</configurationListOrderPriority>
    <factionIconPath>World/WorldObjects/BatarianFlag01</factionIconPath>
    <settlementTexturePath>World/WorldObjects/BatarianFlag01</settlementTexturePath>
    <canMakeRandomly>true</canMakeRandomly>
    <canStageAttacks>true</canStageAttacks>
    <canSiege>true</canSiege>
    <disallowedMemes>
      <li MayRequire="Ludeon.RimWorld.Ideology">Nudism</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Blindsight</li>
    </disallowedMemes>
    <disallowedPrecepts>
      <li MayRequire="Ludeon.RimWorld.Ideology">Slavery_Abhorrent</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Slavery_Horrible</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Slavery_Disapproved</li>
    </disallowedPrecepts>
    <!--<factionNameMaker>NamerFaction</factionNameMaker>-->
    <settlementNameMaker>NamerSettlementOutlander</settlementNameMaker>
    <techLevel>Industrial</techLevel>
    <backstoryCategories>
      <li>BatarianBackstory</li>
    </backstoryCategories>
    <visitorTraderKinds>
      <li>Visitor_Outlander_Standard</li>
    </visitorTraderKinds>
    <caravanTraderKinds>
      <li>Caravan_Neolithic_Slaver</li>
      <li>Caravan_Outlander_CombatSupplier</li>
      <li>Caravan_Outlander_PirateMerchant</li>
    </caravanTraderKinds>
    <baseTraderKinds>
      <li>Base_Outlander_Standard</li>
    </baseTraderKinds>
    <colorSpectrum>
      <li>(1.000, 0.627, 0.478)</li>
      <li>(1.000, 0.714, 0.757)</li>
      <li>(1.000, 0.937, 0.835)</li>
      <li>(0.416, 0.353, 0.804)</li>
      <li>(0.565, 0.933, 0.565)</li>
      <li>(0.957, 0.643, 0.376)</li>
      <li>(1.000, 0.647, 0.000)</li>
      <li>(0.741, 0.718, 0.420)</li>
      <li>(0.251, 0.878, 0.816)</li>
      <li>(0.753, 0.753, 0.753)</li>
      <li>(0.255, 0.412, 0.882)</li>
      <li>(0.596, 0.984, 0.596)</li>
      <li>(0.196, 0.804, 0.196)</li>
      <li>(0.576, 0.439, 0.859)</li>
      <li>(0.855, 0.439, 0.839)</li>
      <li>(0.914, 0.588, 0.478)</li>
      <li>(0.859, 0.439, 0.576)</li>
      <li>(0.686, 0.933, 0.933)</li>
      <li>(0.498, 1.000, 0.831)</li>
    </colorSpectrum>
    <mustStartOneEnemy>false</mustStartOneEnemy>
    <leaderTitle>Chancellor</leaderTitle>
    <allowedArrivalTemperatureRange>
      <min>-15</min>
      <max>40</max>
    </allowedArrivalTemperatureRange>
    <maxPawnCostPerTotalPointsCurve>
      <points>
        <li>(0,50)</li>
        <li>(50, 50)</li>
        <li>(1000, 1000)</li>
        <li>(10000, 10000)</li>
        <li>(100000, 10000)</li>
      </points>
    </maxPawnCostPerTotalPointsCurve>
    <pawnGroupMakers>
      <li>
        <kindDef>Combat</kindDef>
        <options>
          <BatarianColonist01>30</BatarianColonist01>
          <BatarianSoldier01>60</BatarianSoldier01>
          <BatarianBrawler01>40</BatarianBrawler01>
          <BatarianGrenadier01>15</BatarianGrenadier01>
          <BatarianVeteran01>35</BatarianVeteran01>
          <BatarianElite01>20</BatarianElite01>
          <BatarianEliteBrawler01>20</BatarianEliteBrawler01>
          <BatarianChancellor01>1</BatarianChancellor01>
        </options>
      </li>
      <li>
        <kindDef>Trader</kindDef>
        <traders>
          <BatarianTrader01>1</BatarianTrader01>
        </traders>
        <carriers>
          <Dromedary>1</Dromedary>
          <Muffalo>1</Muffalo>
        </carriers>
        <guards>
          <BatarianColonist01>90</BatarianColonist01>
          <BatarianSoldier01>60</BatarianSoldier01>
