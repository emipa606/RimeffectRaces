<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <FactionDef ParentName="FactionBase">
    <defName>QuarianFaction</defName>
    <label>Quarian Colony</label>
    <description>A mixture of exiles, pilgrims, and those who simply left the Flotilla to find a better life, the various Quarian villages on this world still stick together where it counts, despite their differences.</description>
    <pawnsPlural>Quarian</pawnsPlural>
    <fixedName>Quarian Settlements</fixedName>
    <allowedCultures>
      <li>QuarianCulture</li>
    </allowedCultures>
    <settlementGenerationWeight>1</settlementGenerationWeight>
    <requiredCountAtGameStart>1</requiredCountAtGameStart>
    <maxConfigurableAtWorldCreation>9999</maxConfigurableAtWorldCreation>
    <configurationListOrderPriority>10</configurationListOrderPriority>
    <factionIconPath>World/WorldObjects/QuarianFlag01</factionIconPath>
    <settlementTexturePath>World/WorldObjects/DefaultSettlement</settlementTexturePath>
    <canMakeRandomly>true</canMakeRandomly>
    <canStageAttacks>true</canStageAttacks>
    <canSiege>true</canSiege>
    <disallowedMemes>
      <li MayRequire="Ludeon.RimWorld.Ideology">Nudism</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Blindsight</li>
    </disallowedMemes>
    <!--<factionNameMaker>NamerFactionOutlander</factionNameMaker>-->
    <settlementNameMaker>NamerSettlementOutlander</settlementNameMaker>
    <techLevel>Spacer</techLevel>
    <backstoryFilters>
      <li>
        <categories>
          <li>QuarianBackstory</li>
        </categories>
      </li>
    </backstoryFilters>
    <visitorTraderKinds>
      <li>Visitor_Outlander_Standard</li>
    </visitorTraderKinds>
    <caravanTraderKinds>
      <li>Caravan_Outlander_BulkGoods</li>
      <li>Caravan_Outlander_CombatSupplier</li>
      <li>Caravan_Outlander_Exotic</li>
    </caravanTraderKinds>
    <baseTraderKinds>
      <li>Base_Outlander_Standard</li>
    </baseTraderKinds>
    <colorSpectrum>
      <li>(1.000, 0.627, 0.478)</li>
      <li>(1.000, 0.714, 0.757)</li>
      <li>(1.000, 0.937, 0.835)</li>
      <li>(0.416, 0.353, 0.804)</li>
      <li>(0.565, 0.933, 0.565)</li>
      <li>(0.957, 0.643, 0.376)</li>
      <li>(1.000, 0.647, 0.000)</li>
      <li>(0.741, 0.718, 0.420)</li>
      <li>(0.251, 0.878, 0.816)</li>
      <li>(0.703, 0.753, 0.753)</li>
      <li>(0.255, 0.412, 0.882)</li>
      <li>(0.596, 0.984, 0.596)</li>
      <li>(0.196, 0.804, 0.196)</li>
      <li>(0.576, 0.439, 0.859)</li>
      <li>(0.855, 0.439, 0.839)</li>
      <li>(0.914, 0.588, 0.478)</li>
      <li>(0.859, 0.439, 0.576)</li>
      <li>(0.686, 0.933, 0.933)</li>
      <li>(0.498, 1.000, 0.831)</li>
    </colorSpectrum>
    <mustStartOneEnemy>false</mustStartOneEnemy>
    <leaderTitle>High Elder</leaderTitle>
    <allowedArrivalTemperatureRange>
      <min>-45</min>
      <max>40</max>
    </allowedArrivalTemperatureRange>
    <maxPawnCostPerTotalPointsCurve>
      <points>
        <li>(0,50)</li>
        <li>(50, 50)</li>
        <li>(1000, 1000)</li>
        <li>(10000, 10000)</li>
        <li>(100000, 10000)</li>
      </points>
    </maxPawnCostPerTotalPointsCurve>
    <pawnGroupMakers>
      <li>
        <kindDef>Combat</kindDef>
        <options>
          <QuarianColonist01>30</QuarianColonist01>
          <QuarianSoldier01>60</QuarianSoldier01>
          <QuarianVeteran01>35</QuarianVeteran01>
          <QuarianElite01>25</QuarianElite01>
          <QuarianGrenadier01>15</QuarianGrenadier01>
          <QuarianCouncillor01>1</QuarianCouncillor01>
        </options>
      </li>
      <li>
        <kindDef>Trader</kindDef>
        <traders>
          <QuarianTrader01>1</QuarianTrader01>
        </traders>
        <carriers>
          <Muffalo>1</Muffalo>
          <Alpaca>1</Alpaca>
        </carriers>
        <guards>
          <QuarianColonist01>90</QuarianColonist01>
          <QuarianSoldier01>60</QuarianSoldier01>
        </guards>
      </li>
      <li>
        <kindDef>Settlement</kindDef>
        <options>
          <QuarianSoldier01>60</QuarianSoldier01>
          <QuarianVeteran01>25</QuarianVeteran01>
          <QuarianColonist01>50</QuarianColonist01>
          <QuarianElite01>10</QuarianElite01>
          <QuarianCouncillor01>1</QuarianCouncillor01>
        </options>
      </li>
      <li>
        <kindDef>Peaceful</kindDef>
        <maxTotalPoints>300</maxTotalPoints>
        <options>
          <QuarianColonist01>50</QuarianColonist01>
        </options>
      </li>
    </pawnGroupMakers>
  </FactionDef>
</Defs>