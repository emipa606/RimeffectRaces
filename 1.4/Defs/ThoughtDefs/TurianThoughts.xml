<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThoughtDef>
    <defName>TurianInsectMeat</defName>
    <showBubble>true</showBubble>
    <icon>Things/Mote/ThoughtSymbol/Food</icon>
    <durationDays>0.6</durationDays>
    <stages>
      <li>
        <label>ate insect meat</label>
        <description>That meat gave me serious stomach cramps. I think I am dying!</description>
        <baseMoodEffect>-25</baseMoodEffect>
      </li>
    </stages>
  </ThoughtDef>
  <ThoughtDef>
    <defName>TurianRawFood</defName>
    <showBubble>true</showBubble>
    <icon>Things/Mote/ThoughtSymbol/Food</icon>
    <durationDays>0.6</durationDays>
    <stages>
      <li>
        <label>ate raw food</label>
        <description>Eating raw food hurts me on the inside.</description>
        <baseMoodEffect>-20</baseMoodEffect>
      </li>
    </stages>
  </ThoughtDef>
  <ThoughtDef>
    <defName>TurianRottenFood</defName>
    <showBubble>true</showBubble>
    <icon>Things/Mote/ThoughtSymbol/Food</icon>
    <durationDays>0.6</durationDays>
    <stages>
      <li>
        <label>ate rotten food</label>
        <description>That meal was rotten, spirits preserve me!</description>
        <baseMoodEffect>-30</baseMoodEffect>
      </li>
    </stages>
  </ThoughtDef>
</Defs>