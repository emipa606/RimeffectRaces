<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!-- ============================ BatarianBase ========================== -->
  <PawnKindDef Name="BatarianBase" Abstract="True">
    <defaultFactionType>BatarianFaction</defaultFactionType>
    <race>AlienBatarian</race>
    <chemicalAddictionChance>0.15</chemicalAddictionChance>
    <initialWillRange>3~7</initialWillRange>
    <initialResistanceRange>15~23</initialResistanceRange>
    <backstoryCategories>
      <li>BatarianBackstory</li>
    </backstoryCategories>
    <combatEnhancingDrugsChance>0.10</combatEnhancingDrugsChance>
    <combatEnhancingDrugsCount>
      <min>0</min>
      <max>2</max>
    </combatEnhancingDrugsCount>
  </PawnKindDef>
  <!-- ============================ Poor Batarians ========================== -->
  <PawnKindDef Name="BatarianPoorBase" ParentName="BatarianBase" Abstract="True">
    <combatPower>30</combatPower>
    <race>AlienBatarian</race>
    <initialWillRange>2~6</initialWillRange>
    <initialResistanceRange>12~17</initialResistanceRange>
    <itemQuality>Poor</itemQuality>
    <isFighter>false</isFighter>
    <gearHealthRange>
      <min>0.3</min>
      <max>1.0</max>
    </gearHealthRange>
    <apparelMoney>
      <min>150</min>
      <max>300</max>
    </apparelMoney>
    <apparelAllowHeadgearChance>0.3</apparelAllowHeadgearChance>
    <apparelTags>
      <li>Neolithic</li>
    </apparelTags>
    <weaponMoney>
      <min>150</min>
      <max>180</max>
    </weaponMoney>
    <weaponTags>
      <li>MedievalMeleeBasic</li>
    </weaponTags>
    <invNutrition>1.0</invNutrition>
    <inventoryOptions>
      <skipChance>0.75</skipChance>
      <subOptionsChooseOne>
        <li>
          <choiceChance>50</choiceChance>
          <thingDef>Silver</thingDef>
          <countRange>
            <min>25</min>
            <max>70</max>
          </countRange>
        </li>
      </subOptionsChooseOne>
    </inventoryOptions>
  </PawnKindDef>
  <PawnKindDef ParentName="BatarianPoorBase">
    <defName>BatarianSlave01</defName>
    <label>slave</label>
  </PawnKindDef>
