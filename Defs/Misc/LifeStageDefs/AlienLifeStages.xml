<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  
  <LifeStageDef>
    <defName>AlienBabyStage</defName>
    <label>baby</label>
    <bodySizeFactor>0.2</bodySizeFactor>
    <healthScaleFactor>0.2</healthScaleFactor>
    <hungerRateFactor>0.2</hungerRateFactor>
    <marketValueFactor>0.5</marketValueFactor>
    <statFactors>
      <MoveSpeed>0.2</MoveSpeed>
    </statFactors>
  </LifeStageDef>

  <LifeStageDef>
    <defName>AlienChildStage</defName>
    <label>child</label>
    <bodySizeFactor>0.5</bodySizeFactor>
    <healthScaleFactor>0.5</healthScaleFactor>
    <hungerRateFactor>0.6</hungerRateFactor>
    <marketValueFactor>0.9</marketValueFactor>
    <statFactors>
      <MoveSpeed>0.65</MoveSpeed>
    </statFactors>
  </LifeStageDef>

  <LifeStageDef>
    <defName>AlienAdultStage</defName>
    <label>adult stage</label>
    <visible>false</visible>
    <reproductive>true</reproductive>
  </LifeStageDef>

  <LifeStageDef>
    <defName>AlienOldStage</defName>
    <label>old stage</label>
    <visible>false</visible>
            	<statFactors>
      <ImmunityGainSpeed>0.7</ImmunityGainSpeed>
    </statFactors>
	<reproductive>false</reproductive>
  </LifeStageDef>
</Defs>