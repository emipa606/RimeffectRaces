  <ThingDef ParentName="LeatherBase">
    <defName>Leather_Asari</defName>
    <label>asari leather</label>
    <description>Tanned, dried, scraped asari skin. Most people find this material horrifying. Some find it fascinating. While a decent leather, it neither protects nor insulates particularly well.</description>
    <graphicData>
      <color>(184, 184, 222)</color>
    </graphicData>
    <statBases>
      <MarketValue>4.2</MarketValue>
      <StuffPower_Armor_Sharp>0.64</StuffPower_Armor_Sharp>
      <StuffPower_Insulation_Cold>12</StuffPower_Insulation_Cold>
      <StuffPower_Insulation_Heat>12</StuffPower_Insulation_Heat>
    </statBases>
    <stuffProps>
      <color>(184, 184, 222)</color>
      <commonality>0.001</commonality>
    </stuffProps>
  </ThingDef>

